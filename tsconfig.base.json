{
  "compilerOptions": {
    "target": "ESNext",
    "experimentalDecorators": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "skipLibCheck": true
  }
}
